    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://docs.dmarceye.com",
          "icon": "book-open-cover"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
