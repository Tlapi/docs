{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "DMARCeye API docs",
  "colors": {
    "primary": "#FF421E",
    "light": "#07C983",
    "dark": "#FF421E"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Domains",
            "pages": [
              "api-reference/domains/list",
              "api-reference/domains/get",
              "api-reference/domains/create",
              "api-reference/domains/delete",
              "api-reference/domains/verify"
            ]
          },
          {
            "group": "Reports",
            "pages": [
              "api-reference/reports/list",
              "api-reference/reports/get"
            ]
          }
        ]
      }
